{
	"salesforcedx-vscode-core.show-cli-success-msg": false,
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/.sfdx": true
	},
	"codeBuilder.showWelcomePage": false,
	"salesforcedx-vscode-core.push-or-deploy-on-save.enabled": false,
	"salesforcedx-vscode-core.push-or-deploy-on-save.preferDeployOnSave": false,
	"salesforcedx-vscode-core.push-or-deploy-on-save.overrideConflictsOnPush": false,
	"editor.formatOnSave": true,
	"Prettier-SQL.insertSpacesOverride": false,
	"Prettier-SQL.tabSizeOverride": 4,
	"Prettier-SQL.expressionWidth": 250,
	"Prettier-SQL.keywordCase": "upper"
}
